{
    "version": 2,
    "builds": [
        { "src": "index.js", "use": "@now/node",
            "config":{
                "includeFiles": ["swagger.yaml"]
            }
        }
    ],
    "routes":[
        {
            "src": "/api/(.*)",
            "dest":"/api/$1"
        },
        {
            "src": "/(.*)",
            "dest":"/index.js"
        }
    ]
}