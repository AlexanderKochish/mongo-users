{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" },
        { "src": "*.yml", "use": "@vercel/yaml" }
    ],

    "routes": [
        { "src": "/", "dest": "./index.js" },
        { "src": "/users", "methods": ["POST","GET"], "dest": "./index.js" },
        { "src": "/", "dest": "./swagger.yml" }
    ]
}