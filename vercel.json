{
    "version": 2,
    "build": {
        "functions": {
            "runtime":"node14.x"
        }
    },
    "routes":[
        {
            "handle":"filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/docs/$1"
        },
        {
            "src":".*",
            "dest":"/index.js"
        }
    ]
}